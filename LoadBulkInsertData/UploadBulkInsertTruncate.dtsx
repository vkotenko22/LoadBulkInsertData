         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
		{
            // TODO: Add your code here
            string SMTPServer = "smtp.gmail.com";
            int SMTPPort = 587;
            bool EnableSSL = true;
            string dataFilesPath = Dts.Variables["DataFilePath"].Value.ToString();
            string databaseName = Dts.Variables["DatabaseName"].Value.ToString();

            SmtpClient objSmtp = new SmtpClient(SMTPServer);
            objSmtp.Credentials = new NetworkCredential("kotenko121@gmail.com", "ffedodchpwnfagrm");
            objSmtp.EnableSsl = EnableSSL;
            objSmtp.Port = SMTPPort;

            MailMessage message = new MailMessage();
            message.From = new MailAddress("kotenko121@gmail.com");
            message.To.Add("vk@l-o-g-i-c.com");
            message.Subject = "Uploading Success";
            message.Body = $"All Files from folder {dataFilesPath} were uploaded to {databaseName} database";

            objSmtp.Send(message);

            //MessageBox.Show("Email sended successfully");

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region ScriptResults declaration
